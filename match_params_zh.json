{
    "map_matcher": "SimpleMatcher",
    "stop_matcher": "NaiveStopMatcher",
    "use_rtree": false,
    "exit_filter": "way ['highway'] not in ('service', 'unknown') and not way ['highway'].endswith ('_link')",
    "default_name": "无名路",
    "forward_angle": 45,
    "follow_link": "%n辅路",
    "snap_gpx": 5,
    "process_divided": {
        "angle": 10,
        "length": 50,
        "same_name": true,
        "apply_filter": true,
        "enabled_cases": [1, 2, 3, 4]
    },
    "visualize": true,
    "hw_priority": {
        "busway": 7,
        "motorway": 6,
        "trunk": 5,
        "primary": 4,
        "secondary": 3,
        "tertiary": 2,
        "unclassified": 1,
        "residential": 1
    },
    "matcher_params": {
        "max_dist": 100,
        "max_dist_init": 100,
        "min_prob_norm": 0.001,
        "non_emitting_length_factor": 0.5,
        "obs_noise": 50,
        "obs_noise_ne": 75,
        "dist_noise": 100,
        "non_emitting_states": true,
        "max_lattice_width": 5,
        "avoid_goingback": true
    },
    "display_params": {
        "display": "SimpleTextDisplay",
        "duration": 10,
        "transfer_separator": " ",
        "bar_reverse": true,
        "use_reference": false
    },
    "visu_template": "visualization_template.html"
}