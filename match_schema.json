{
    "$schema": "http://json-schema.org/draft/2020-12/schema",
    "title": "match_params",
    "type": "object",
    "properties": {
        "map_matcher": {
            "type": "string",
            "enum": ["SimpleMatcher", "DistanceMatcher"]
        },
        "stop_matcher": {
            "type": "string",
            "enum": ["NaiveStopMatcher"]
        },
        "use_rtree": {
            "type": "boolean"
        },
        "exit_filter": {
            "oneOf": [
                {
                    "type": "string"
                },
                {
                    "type": "boolean",
                    "enum": [false]
                }
            ]
        },
        "default_name": {
            "type": "string"
        },
        "forward_angle": {
            "type": "number",
            "minimum": 0,
            "maximum": 180
        },
        "follow_link": {
            "type": "string"
        },
        "snap_gpx": {
            "oneOf": [
                {
                    "type": "number",
                    "minimum": 0
                },
                {
                    "type": "boolean",
                    "enum": [false]
                }
            ]
        },
        "process_divided": {
            "oneOf": [
                {
                    "type": "object",
                    "properties": {
                        "angle": {
                            "type": "number",
                            "minimum": 0,
                            "maximum": 180
                        },
                        "length": {
                            "type": "number",
                            "minimum": 0
                        },
                        "same_name": {
                            "type": "boolean"
                        },
                        "apply_filter": {
                            "type": "boolean"
                        },
                        "enabled_cases": {
                            "type": "array",
                            "items": {
                                "type": "integer",
                                "enum": [1, 2, 3, 4]
                            },
                            "uniqueItems": true,
                            "minItems": 1,
                            "maxItems": 4
                        }
                    },
                    "additionalProperties": false,
                    "required": ["angle", "length", "same_name", "apply_filter", "enabled_cases"]
                },
                {
                    "type": "boolean",
                    "enum": [false]
                }
            ]
        },
        "visualize": {
            "type": "boolean"
        },
        "hw_priority": {
            "type": "object",
            "additionalProperties": {
                "type": "integer"
            }
        },
        "matcher_params": {
            "type": "object"
        },
        "display_params": {
            "type": "object",
            "oneOf": [
                {
                    "type": "object",
                    "properties": {
                        "display": {
                            "type": "string",
                            "enum": ["SimpleTextDisplay"]
                        },
                        "duration": {
                            "type": "number",
                            "minimum": 0
                        },
                        "transfer_separator": {
                            "type": "string"
                        },
                        "bar_reverse": {
                            "type": "boolean"
                        },
                        "use_reference": {
                            "type": "boolean"
                        }
                    },
                    "additionalProperties": false,
                    "required": ["display", "duration", "transfer_separator", "bar_reverse", "use_reference"]
                }
            ]
        },
        "visu_template": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "minProperties": 14
}